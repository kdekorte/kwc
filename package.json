    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "electron": "^1.4.15",
    "express": "^4.14.0",
    "react": "^15.4.2",
